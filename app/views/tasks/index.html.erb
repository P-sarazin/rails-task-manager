<div class="container">
  <div class="row">
    <div class="col">
      <h1>My tasks</h1>

      <ul>
        <% @tasks.each do |task| %>
          <li> <input type="checkbox"><%= link_to task.title, task_path(
            task) %></li>
        <% end %>
      </ul>
      <ul>
        <li><%= link_to "Create task", new_task_path %></li>
      </ul>

      <ul>
        <li><%= link_to "Delete task", task_path %></li>
      </ul>

       <ul>
        <li><%= link_to "Edit task", edit_task_path %></li>
      </ul>

    </div>
   </div>
</div>



